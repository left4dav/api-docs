{
  "kind": "collectionType",
  "collectionName": "endpoints",
  "info": {
    "singularName": "endpoint",
    "pluralName": "endpoints",
    "displayName": "Endpoint",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uid": {
      "type": "uid",
      "required": true
    },
    "isObject": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "title": {
      "type": "string"
    },
    "method": {
      "type": "enumeration",
      "enum": [
        "GET",
        "POST",
        "PUT"
      ],
      "required": true,
      "default": "GET"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "allParamertersAreOptional": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "bodyParameters": {
      "type": "component",
      "repeatable": true,
      "component": "endpoint-parameters.request-body"
    },
    "pathParameters": {
      "type": "component",
      "repeatable": true,
      "component": "endpoint-parameters.request-path"
    },
    "queryParameters": {
      "type": "component",
      "repeatable": true,
      "component": "endpoint-parameters.request-query"
    },
    "sdkMethods": {
      "type": "component",
      "repeatable": true,
      "component": "endpoint-sdk-methods.sdk-methods"
    },
    "sliceZone": {
      "type": "dynamiczone",
      "components": [
        "admonition.admonition"
      ]
    }
  }
}
